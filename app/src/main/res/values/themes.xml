    </style>

    <style name="Theme.SmartPrinter" parent="Base.Theme.SmartPrinter" />


    <style name="CustomShapeAbleImageViewTheme">
        <item name="shapeAppearanceOverlay">@style/CustomShapeAppearance</item>
        <item name="strokeWidth">0dp</item>
        <item name="android:backgroundTint">@color/app_color</item>
    </style>
    <style name="CustomShapeAppearance" parent="ShapeAppearance.MaterialComponents.SmallComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">@dimen/_5sdp</item>
    </style>

    <style name="AppTheme_switch" parent="Theme.MaterialComponents.DayNight.NoActionBar">

        <item name="colorControlActivated">#396AFC</item>
        <item name="colorSwitchThumbNormal">#ABACB4</item>
        <item name="android:colorForeground">@color/white</item>

    </style>
    <style name="DialogAnimationInOut">
        <item name="android:windowEnterAnimation">@anim/animate_in_out_enter</item>
        <item name="android:windowExitAnimation">@anim/animate_in_out_exit</item>
    </style>

    <style name="RatingBar" parent="Theme.AppCompat">
        <item name="colorControlNormal">@color/normal</item>
        <item name="colorControlActivated">@color/active_color</item>

    </style>

